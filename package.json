{
  "name": "imageboard",
  "version": "1.0.0",
  "description": "an imageboard with elm, graphql, postgraphile and postgresql",
  "main": "index.js",
  "type": "module",
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": ">= 22.0.0",
    "npm": ">= 10.0.0"
  },
  "scripts": {
    "start": "node index",
    "build": "elm make src/Main.elm --output=public/main.js",
    "build:debug": "elm make --debug src/Main.elm --output=public/main.js",
    "format": "elm-format src/Main.elm",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "postgraphile": "^4.14.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.2",
    "elm": "^0.19.1-6",
    "elm-format": "^0.8.7"
  }
}
